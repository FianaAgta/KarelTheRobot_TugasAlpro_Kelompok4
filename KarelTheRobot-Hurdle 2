#membuat fungsi agar robot bisa berbelok ke kanan
def turn_right():
    turn_left()
    turn_left()
    turn_left()
#fungsi untuk menjalankan robot
def lewat_tembok():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
#me-repeat gerakan robot jika robot belum mencapai titik finish
while (not at_goal()):
    lewat_tembok()
